{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {},
  "rules": {
    "max-line-length": [
      true,
      200
    ],
    "import-name": false,
    "align": false,
    "import-spacing": true,
    "indent": [
      true,
      "spaces",
      4
    ],
    "interface-over-type-literal": true,
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "static-method",
          "instance-field",
          "instance-method"
        ]
      }
    ],
    "no-misused-new": true,
    "no-non-null-assertion": true,
    "no-shadowed-variable": true,
    "no-this-assignment": [
      false
    ],
    "eofline": true,
    "deprecation": true,
    "max-classes-per-file": [
      true,
      1
    ],
    "prefer-const": true,
    "arrow-return-shorthand": [
      true,
      "multiline"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "class-name": true,
    "encoding": true,
    "jsdoc-format": [
      true
    ],
    "no-redundant-jsdoc": [
      true
    ],
    "no-trailing-whitespace": [
      true,
      "ignore-blank-lines",
      "ignore-jsdoc"
    ],
    "prefer-method-signature": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "ter-arrow-parens": [
      false
    ],
    "ter-indent": [
      true,
      4
    ],
    "object-curly-spacing": true
  }
}
